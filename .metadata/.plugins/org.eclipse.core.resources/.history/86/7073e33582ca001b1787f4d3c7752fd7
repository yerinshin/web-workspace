<script>
	let d = new Date()
	
	let hour = d.getHours()
	
	if(hour < 12) {
		alert("오전시간입니다")
	} else if(hour < 18){
		alert("오후시간입니다")
	} else {
		alert("저녁시간입니다")
	}
		
</script>
