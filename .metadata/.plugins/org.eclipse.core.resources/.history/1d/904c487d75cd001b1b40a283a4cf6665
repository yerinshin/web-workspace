	class Person {
		constructor(name, phone, addr) {
			this._name = name
			this._phone = phone
		}
		
		info() {
			alert(`name : ${ this.name }\nphone : ${ this.phone }\naddr : ${this.addr}`)
		}
		
		
	}
	

	let p = new Person("홍길동", "010-1111-2222", "경기도 광명시") 	//new할때만 prototype 가능 --> let p = {name:'hong'} : 이경우 p.prototype 사용 불가
	p.info()
	console.log(p)
	
