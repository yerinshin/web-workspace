<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>
	let person = {
		'name' : '홍길동',
		'phone' : '010-1111-2222',
		'addr' : '경기도 광명시',
		'info' : function(){
			alert('name : '+ this.name + '\n'
					+ 'phone : ' + this.phone + '\n'
					+ 'addr : ' + this.addr )	//this 키워드 생략 불가
		}
	}
	
	person.info()
</script>
</head>
