<title>Insert title here</title>
<script>

	// 생성자 함수
	function Person(name, phone, addr) {
		this.name = name
		this.phone = phone
		this.addr = addr
		this.info = function() {
			let msg = ''
				for(let key in this){
					if(key != 'info')
					msg += key + ' : ' + this[key] + '\n'
				}
				alert(msg)
		}
	}
	
	new Person('홍길동', '010-1111-2222', '경기도 광명시') 
</script>
