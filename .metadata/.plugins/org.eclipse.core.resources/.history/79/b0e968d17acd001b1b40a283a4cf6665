<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>
	class Person {
		constructor(name, phone, addr) {
			this._name = name	
			this._phone = phone
			this._addr = addr
		}
		
		info() {
			alert(`name : ${ this.name }\nphone : ${ this.phone }\naddr : ${this.addr}`)
		}
		
		get name(){				//getter 없으면 p.name으로 접근 할수 있음(원래 p._name으로 접근해야함. )
			return this._name
		}
		
		set name(name) {
			this._name = name
		}
		
		get phone() {
			return this._phone
		}
		
		set phone(phone) {
			this._phone
		}
		
		get addr() {
			return this._addr	
		}
		
		set addr(addr) {
			this._addr = addr
		}
		
	}
	
	class Doctor {
		
	}

</script>
</head>
<body>

</body>
</html>