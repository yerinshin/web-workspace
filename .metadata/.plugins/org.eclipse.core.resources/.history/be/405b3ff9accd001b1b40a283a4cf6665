<script>
	window.onload = function() {
		let h1Tag = document.createElement('h1')	
		let text = document.createTextNode('hello')
		h1Tag.appendChild(text)				//h1 태그 안에 text넣기
	/* console.log(h1Tag) */
	/* 	console.log(text) */
		document.body.appendChild(h1Tag)	//body 안에 h1태그 넣기
		
		let linkTag = document.createElement('a')
		
		
		let imgTag = document.createElement('img')
		imgTag.setAttribute('src', 'https://t1.daumcdn.net/daumtop_chanel/op/20200723055344399.png')
		console.log(imgTag)
		
		document.body.appendChild(imgTag)	//body 안에 img태그 넣기
		
	}
	
</script>
