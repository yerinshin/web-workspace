<title>Insert title here</title>
<script>
	/*
		JSON.parse()	: 문자열을 자바스크립트객체 변환
		JSON.stringify	: 자바스크립트객체를 문자열로 변환
	*/
	
	let person = {
		'name' : '홍길동',
		'phone' : '010-1111-2222',
		'addr' : '경기도 광명시'
		}
	
	console.log(person)
	console.log(JSON.stringify(person))
</script>
