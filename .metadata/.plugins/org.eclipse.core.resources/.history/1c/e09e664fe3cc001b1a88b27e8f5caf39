<script>

	function clone(obj) {
		let object = {}
		for(let key in obj)
	}
	
	
	let person = {
			name : '홍길동',
			phone : '010-1111-2222'
	}
	
	
	//얕은복사
//	let person2 = person 	
	let person2 = clone(person)	


	person2.phone = '010-5555-6666'
	
	console.log(person)
	console.log(person2)
	
