<script>
	
	function returnFunction() {
		
		return function() {
			alert('return function...')
		}
	}
	
//	let ret = returnFunction()
//	ret()
	
	returnFunction()()	
	
	function test(name) {
		var output = 'Hello ' +name + "!!!"
	}
	
	//closable : 함수가 끝나도 리턴값을 잠시 유지시키기 위해서
	function test(name) {
		var output = 'Hello ' +name + "!!!"
		return function() {
			alert(output)
		}
	}
	
//	alert(output)
	
	test('홍길동')
	
	
	
</script>
