<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>
	//함수이름(변수, 변수, ...)
	//메소드 오버로딩은 지원되지 않음 -> 그냥 덮어쓰기 됨
	
	function testFunction() {
		alert('testFunction() 호출...')
	}
	
	function testFunction01(data) {
		alert('data : ' + data)
	}
	
	function testFunction02(a, b){
		alert(`a : $ { a }, b : ${ b }`)
	}
	
	 function add(a, b){
		 return a + b
	 }
	 
	 let sum = add(10, 20)
	 alert('총합 : ' + sum)
	
	testFunction02('A', 10)
	testFunction01(123)
	testFunction()
</script>
</head>
