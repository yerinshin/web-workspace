<script>
	function Person(name, phone, addr) {
		this.name = name
		this.phone = phone
		this.addr = addr
	}
	
	Person.prototype.hobby = '영화감상'
	
	
	Person.prototype.info = function() {
		let msg = ''
		for(let key in this) {
			if(key != 'info')
				msg += key + ' : ' + this[key] + '\n' 
		}
		
		alert(msg)
	}
	
	function Doctor(name, phone, addr, major) {
		this.name = name
		this.phone = phone
		this.addr = addr
		this.major = major
	}
	
