<title>Insert title here</title>
<script>
	function Person(name, phone, addr) {
		this.name = name
		this.phone = phone
		this.addr = addr
	}
	
	Person.prototype.hobby = '영화감상'
	Person.prototype.info = function() {
		let msg = ''
		for(let key in this) {
			msg += key + ' : ' + this[key] + '\n' 
		}
		
		alert(msg)
	}
</script>
