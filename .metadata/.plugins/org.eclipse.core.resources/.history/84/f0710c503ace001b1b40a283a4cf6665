<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>

	let num = prompt("10 ~ 100 사이의 숫자를 입력하세요 : ")
	
	for(i = 1; i <= num; i++) {

		if(i<10) {
			if(i % 3 == 0){
				document.write( '짝<br>')				
			}else {
				document.write( i + '<br>')
			}
			
		} else {
			
			
			let tens = 	Math.floor(i/10)	//10의 자리수
			let multipleOfTen = false		//10의 배수인가
			let ThreeSixNine = false		//369해당
			let tensTSN = false				//10의자리 3,6,9인가
			console.log(tens)
			
			if( (i % 10) % 3 == 0 ){
				ThreeSixNine = true
			}
			
			if( i % 10 == 0 ){
				multipleOfTen = true
			}
			
			if(tens % 3 ==0) {
				tensTSN = true
			}
			
			if(!ThreeSixNine && !multipleOfTen && !tensTSN){
			document.write( i + '<br>')
		}
		
	 	if(ThreeSixNine && !multipleOfTen){
			document.write('짝<br>')
		} 
		
		if (!tensTSN && multipleOfTen) {
			for(j = 1; j<= tens; j ++){
				document.write('뽀')
			}
			document.write('숑<br>')
		}
		
 		if(tensTSN && multipleOfTen) {
			document.write('짝')
			for(j = 1; j<= tens; j ++){
				document.write('뽀')
			}
			document.write('숑<br>')
		}  
		
		if(!multipleOfTen && tensTSN) {
			document.write('짝<br>')
		}   
	}
	}
</script>
</head>
<body>


</body>
</html>